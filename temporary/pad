noses=mouth_cascade.detectMultiScale(roi_gray, 1.3, 5)
for (x,y,w,h) in noses:
    cv2.putText(img=roi_color,text="mulut",org=(x,y),fontFace=cv2.FONT_HERSHEY_SIMPLEX,fontScale=0.5,color=(0,255,0),thickness=1)
    cv2.rectangle(roi_color,(x,y),(x+w,y+h),(0,255,0),1)
smiles=nose_cascade.detectMultiScale(roi_gray, 1.3, 5)
for (x,y,w,h) in smiles:
    print(str(smiles.shape[0]))
    cv2.putText(img=roi_color,text="senyum",org=(x,y),fontFace=cv2.FONT_HERSHEY_SIMPLEX,fontScale=0.5,color=(0,0,255),thickness=1)
    cv2.rectangle(roi_color,(x,y),(x+w,y+h),(0,0,255),1)
